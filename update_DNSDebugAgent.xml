<?xml version="1.0" encoding="UTF-8"?>
<WinCollectScript version="10.0">
    <Update objPath="LocalSources(Name=Local)" >
        <Parameter name="Enabled" value="true" />
        <Source Channel="Application" Enabled="true" />
        <Source Channel="System" Enabled="true" />
        <Source Channel="Security" Enabled="true" />
        <Source Name="DNS Debug" IncludeDetails="true" Type="WindowsDNS" >
            <Parameter name="FilenamePattern" value="{filenamePattern}" />
            <Parameter name="FileOpenMode" value="{FileOpen}" />
            <Parameter name="RootDirectory" value="{RootDirectory}" />
        </Source>    
    </Update>
</WinCollectScript>
